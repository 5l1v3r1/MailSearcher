<script>
function course_descr(c) {
   var descr = [];
   if (c.authors) { descr.push(c.authors); };
   if (c.organisation) { descr.push(c.organisation); };
   return descr.join("; ");
};

$(document).ready(function() {
 $('#expand').click(function(){
    $('#collapsible').slideToggle('slow');
 });
 var courses = {};
 var courseLabels = [];
 $('.ajax-typeahead#search').typeahead({
     source : function(query, process) {
        return $.get('/courses/simple_search.json', {
           q : query
        }, function(res) {
           courses = {};
           courseLabels = [];
           $.each(res.data, function(i,n) {
               var label = n.id;
               courseLabels.push(label);
               courses[label] = {
                 url:  n.url,
                 title: n.title,
                 descr: n.descr,
                 photo: n.photo,
                 authors: n.authors,
                 organisation: n.organisation,
               };
           });
           return process(courseLabels);
        });
     },
     minLength : 2,
     items : 16,
     select : function(event, ui) {
         $('.ajax-typeahead#search').val(ui.item.value);
         $('form#search').submit();
     },
     matcher: function () { return true; },
     updater: function (item) {
         document.location.href = courses[item].url;
     },
     highlighter: function(item) {
         var c = courses[item];
         var itm = ''
            + "<div class='typeahead_wrapper'>"
            + "<span class='typeahead_labels'>"
            + "<span class='typeahead_primary'>" + c.title + "</span>"
            + "<span class='typeahead_secondary'>" + course_descr(c) + "</span>"
            + "</span>"
            + "<span class='typeahead_photo_wrapper'><img class='typeahead_photo' src='" + c.photo + "' title='" + c.title + "'/></span>"
            + "</div>";
         return itm;
     },
 });
});
</script>

