<script>
  $(document).ready(function() {
   $('#loading-indicator').hide();
   $(document)  
   .ajaxStart(function() {
     $('#loading-indicator').show();
     $('#submit_review').prop('disabled', 'disabled');
   })
   .ajaxStop(function() {
     $('#loading-indicator').hide();
     $('#submit_review').prop('disabled', false);
   });
  });
</script>
<script>
  $(document).ready(function() {
   $('#fav_button').tooltip({'placement': 'right', 'title': 'Войдите под своей учётной записью, чтобы добавить курс в избранное'});
  });
</script>
<script>
  function activate_reviews(editor_mode_in_addition_to_normal) {
   var path   = '/assets';
   var number = 5;
   var half   = true;
   var hints  = ['плохо', 'так себе', 'средне', 'хорошо', 'замечательно'];
   var notrated = 'без оценки';
   if (editor_mode_in_addition_to_normal) {
    $('#star').raty({
     path: path,
     number: number,
     half: half,
     scoreName: 'stars',
     hints: hints,
     noRatedMsg: notrated,
    });
   }
   $('.review-star').raty({
     readOnly: true,
     path: path,
     number: number,
     half: half,
     hints: hints,
     noRatedMsg: notrated,
     score: function() {
       return $(this).attr('data-score');
     },
     space: false,
   });
   $('.delete_review').bind('ajax:success', function(evt, data, status, xhr){
     $(this).parents('.review').fadeOut('slow');
    });
  };
  $(document).ready(function() {
   $('#review_form')
    .bind('ajax:error', function(){
      alert("Error!");
    });
   activate_reviews(true);
  });
</script>

<div class='row-fluid'>
<div class='span10'>
<div class="modal hide fade" id="newcourselist">
  <div class="modal-header" id="modal_header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3>Название подборки</h3>
  </div>
  <div class="modal-body" id="modal_body">
    <input type="text" id="listname" placeholder="Введите название вашей новой подборки" class="span12">
    <textarea id="description" class="span12" rows="5"></textarea>
    <label class="checkbox">
      <input type="checkbox" id="is_private">Скрытая
    </label>
    <input type="hidden" id="listname_permalink" name="listname_permalink">
    <input type="hidden" id="listname_url" name="listname_url">
  </div>
  <div class="modal-footer">
    <a href="#" class="btn" onclick="hide_modal();">Отменить</a>
    <a href="#" class="btn btn-primary" onclick="create_new_courselist();">Сохранить</a>
  </div>
</div>

<script type="text/javascript">
function hide_modal() {
  $('#error_alert').remove();
  $('#listname_permalink').val();
  $('#listname_url').val();
  $('#listname').val('');
  $('#description').val('');
  $('#is_private').val('');
  $('#newcourselist').modal('hide');
};
function show_modal(permalink, url) {
  $('#error_alert').remove();
  $('#listname_permalink').val(permalink);
  $('#listname_url').val(url);
  $('#newcourselist').modal();
};
function add_to_courselist(courselist_id, permalink, url) {
  $.post('/courselists/create_quick',
    { permalink: permalink, url: url, courselist_id: courselist_id },
    function (data, textStatus, jqXHR) {
      if (data.success) {
        bootbox.alert(data.message);
      } else {
        bootbox.alert('Error');
      }
    }
  );
};
function create_new_courselist() {
  $('#error_alert').remove();
  var listname = $('#listname').val();
  var description = $('#description').val();
  var is_private = $('#is_private').is(':checked');
  var course_permalink_to_add = $('#listname_permalink').val();
  var course_url_to_add = $('#listname_url').val();
  $.post('/courselists/create_quick',
    { permalink: course_permalink_to_add, url: course_url_to_add, title: listname, description: description, is_private: is_private ? 1 : 0 },
    function (data, textStatus, jqXHR) {
      if (data.success) {
        $('<div class="alert alert-success">'+data.message+'</div>')
         .prependTo($('#modal_body'))
         .hide()
         .fadeIn(1000, function() {
           $(this).remove();
           $('#newcourselist').modal('hide');
           $('#listname').val('');
           $('#description').val('');
           $('#is_private').val('');
         });
         window.location.reload(true);
      } else {
        $('<div class="alert alert-error" id="error_alert">'+data.error+'</div>')
         .prependTo($('#modal_body'))
         .hide()
         .fadeIn(1000);
      }
    }
  );
};
</script>

